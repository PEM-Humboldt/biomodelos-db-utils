{
    "type": "object",
    "properties":{
        "acceptedNameUsage": { "type": "string"},	
        "altitudinalOutlier": { "type": ["boolean", "null"], "default" : null },
        "basisOfRecord": { "type": ["string", "null"], "default": null },
        "catalogNumber": { "type": ["string", "null"], "default": null},
        "cellID": { "type": ["number", "null"], "default": null },
        "collectionCode": { "type": ["string", "null"], "default": null },
        "consistentAltitude": { "type": ["boolean" , "null"], "default" : null },
        "continent": { "type": "string", "default": "" },
        "contributedRecord": { "type": ["boolean" , "null"], "default" : null },
        "coordinateUncertaintyInMeters": { "type": ["number", "null"], "default" : null },
        "correctCountry": { "type": ["boolean" , "null"], "default" : null },
        "correctCounty": { "type": ["boolean" , "null"], "default" : null },
        "correctStateProvince": { "type": "boolean" , "default" : "" },
        "country": { "type": "string", "default" : "" },
        "county": { "type": ["string", "null"], "default" : "" },
        "createdAlt": { "type": ["number", "null"], "default" : null },
        "createdBasisOfRecord": { "type": ["string", "null"] },
        "createdLat": { "type": ["number", "null"], "default" : null },
        "createdLocality": { "type": ["string", "null"], "default" : null},
        "createdLon": { "type": ["number", "null"], "default" : null },
        "createdSource": { "type": ["string", "null"], "default" : null },
        "createdTaxID": { "type": ["number", "null"] },
        "day": { "type": ["number", "null"], "default" : null , "minimum": 1, "maximum": 31},
        "dbDuplicate": { "type": "boolean" , "default" : false },
        "decimalLatitude": { "type": "number", "minimum": -90, "maximum": 90},
        "decimalLongitude": { "type": "number", "minimum": -180, "maximum": 180},
        "demAltitude": { "type": ["number", "null"], "default" : null },
        "diferenceInAltitude": { "type": ["number", "null"], "default" : null },
        "dist2KnowRange": { "type": ["number", "null"], "default" : null },
        "downloadDate": { "type": ["string", "null"], "default" : null, "format": "date-time" },
        "environmentalOutlier": { "type": ["boolean" , "null"], "default" : null },
        "errorProbability": { "type": ["number", "null"], "default" : null },
        "eventDate": { "type": ["string", "boolean" , "null"], "default" : null, "format": "date-time"},
        "externallyCreated": { "type": "boolean" , "default" : false },
        "field": { "type": ["string", "null"]},
        "hasLocality": { "type": ["boolean" , "null"], "default" : null },
        "insideKnownDistribution": { "type": "boolean" , "default" : false},	
        "institutionCode": { "type": ["string", "null"], "default" : null },
        "interpretedElevation": { "type": ["number", "null"], "default" : null },
        "inUrbanArea": { "type": ["boolean" , "null"], "default" : null },
        "lowUncertainty": { "type": ["boolean" , "null"], "default" : null },
        "maximumElevationInMeters": { "type": ["number", "null"], "default" : null , "minimum": 0},
        "minimumElevationInMeters": { "type": ["number", "null"], "default" : null , "minimum": 0},
        "month": { "type": ["number", "null"], "default" : null , "minimum": 1, "maximum": 12},
        "occurrenceID": { "type": "string"},
        "origin": { "type": "string", "default" : "native", "enum" : ["native", "introduced", "vagrant"]},
        "override": { "type": ["boolean" , "null"] },
        "presence": { "type": "string", "default" : "extant", "enum" : ["extant", "probablyExtant", "possiblyExtinct", "extinct"]},
        "previousFieldValue": { "type": ["string", "null"]},
        "privateData": { "type": "number", "default" : 0, "enum" : [0, 1, 2]},
        "recordedBy": { "type": ["string", "null"], "default" : null },
        "recordStatus": { "type": ["boolean" , "null"], "default" : null },
        "resourceFolder": { "type": "string", "default": "" },	
        "resourceIncorporationDate": { "type": ["string", "date"], "default": "", "format": "date-time"},
        "resourceName": { "type": "string", "default": "" },
        "season": { "type": "string", "default": "resident", "enum" : ["resident", "breedingSeason", "nonBreedingSeason", "passage"]},
        "source": { "type": ["string", "null"], "default" : null },
        "sourceLayer": { "type": ["string", "null"], "default" : null },
        "spatialDuplicated": { "type": "boolean", "default": false },
        "species": { "type": "string", "default" : "" },
        "speciesOriginal": { "type": "string", "default": "" },
        "stateProvince": { "type": ["string", "null"], "default": null },
        "suggestedCountry": { "type": ["string", "null"], "default" : null },
        "suggestedCounty": { "type": ["string", "null"], "default" : null },
        "suggestedMunicipality": { "type": ["string", "null"], "default" : null },
        "suggestedStateProvince": { "type": ["string", "null"], "default" : null },
        "taxID": { "type": "number"},
        "url": { "type": ["string", "null"], "default" : null},
        "use": { "type": "boolean", "default" : true },
        "verbatimElevation": { "type": ["number","string", "null"], "default" : null },
        "verbatimLocality": { "type": ["string", "null"], "default" : null },
        "visualizationPrivileges": { "type": "number", "default" : 0, "enum" : [0, 1, 2]},
        "year": { "type": ["number", "null"], "default" : null, "minimum": 1800}
    },
    "required": [
        "acceptedNameUsage",
        "decimalLatitude",
        "decimalLongitude",
        "privateData",
        "occurrenceID",
        "resourceFolder",
        "taxID",
        "visualizationPrivileges",
        "resourceName"
    ],
    "allOf": [
        {
            "if": {
                "properties": { "month": { "const": 2 } }
            },
            "then": {
                "properties": {
                "day": { "maximum": 29 }
                }
            }
            },
            {
            "if": {
                "properties": { "month": { "enum": [4, 6, 9, 11] } }
            },
            "then": {
                "properties": {
                "day": { "maximum": 30 }
                }
            }
        }
    ],
    "additionalProperties": false
}
